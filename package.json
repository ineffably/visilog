{
  "name": "llm-dom2file-logger",
  "version": "0.7.1-beta.1",
  "description": "A simple logging solution for your dev env tools like VITE and Webpack that allows your LLM model to \"see\" your browser logs without using MCP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "validate": "npm run test && npm run build",
    "clean": "rm -rf dist coverage node_modules/.cache",
    "prepack": "npm run build:clean",
    "prepublishOnly": "npm run validate",
    "postpublish": "echo 'Package published successfully!'",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "release:dry": "npm pack --dry-run",
    "audit:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update"
  },
  "keywords": [
    "websocket",
    "logging",
    "development",
    "vite",
    "webpack",
    "plugin",
    "debug",
    "real-time",
    "console"
  ],
  "author": "Chad Neff <ccneff@gmail.com> (https://github.com/ineffably)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/websocket-dev-logger.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/websocket-dev-logger/issues"
  },
  "homepage": "https://github.com/yourusername/websocket-dev-logger#readme",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "audit-ci": "^7.1.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "require": "./dist/client/websocket-logger.js",
      "types": "./dist/client/websocket-logger.d.ts"
    },
    "./server": {
      "require": "./dist/server/websocket-logger-server.js",
      "types": "./dist/server/websocket-logger-server.d.ts"
    },
    "./vite": {
      "require": "./dist/plugins/vite-plugin.js",
      "types": "./dist/plugins/vite-plugin.d.ts"
    },
    "./webpack": {
      "require": "./dist/plugins/webpack-plugin.js",
      "types": "./dist/plugins/webpack-plugin.d.ts"
    }
  }
}
